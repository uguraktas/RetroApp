{
  "name": "native",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start --clear",
    "android": "expo run:android --no-build-cache --device",
    "ios": "expo run:ios --no-build-cache --device",
    "prebuild": "expo prebuild --clean",
    "web": "expo start --web",
    "eas:ios": "eas build --platform ios",
    "eas:android": "eas build --platform android",
    "eas:all": "eas build --platform all",
    "eas:ios:local": "npx eas build --platform ios --local --profile development",
    "eas:android:local": "npx eas build --platform android --local --profile development",
    "eas:submit:ios": "eas submit -p ios",
    "eas:submit:android": "eas submit -p android",
    "eas:submit:all": "eas submit --platform all"
  },
